# Production Environment Variables
# DO NOT commit sensitive values to version control

# API Configuration
API_BASE_URL=https://your-production-api.com
API_TIMEOUT=30000

# App Configuration
NODE_ENV=production
EXPO_PUBLIC_ENV=production

# Feature Flags
EXPO_PUBLIC_ENABLE_DEV_TOOLS=false
EXPO_PUBLIC_ENABLE_FLIPPER=false
EXPO_PUBLIC_ENABLE_REACTOTRON=false

# Analytics (if implemented)
# EXPO_PUBLIC_ANALYTICS_ENABLED=true
# EXPO_PUBLIC_ANALYTICS_KEY=your-analytics-key

# Error Tracking (if implemented)
# EXPO_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Performance Monitoring
EXPO_PUBLIC_PERFORMANCE_MONITORING=true

# Cache Configuration
EXPO_PUBLIC_CACHE_SIZE=50
EXPO_PUBLIC_IMAGE_CACHE_SIZE=100

# Location Services
EXPO_PUBLIC_LOCATION_ACCURACY=high
EXPO_PUBLIC_LOCATION_TIMEOUT=15000

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_SOUND=notification.wav
EXPO_PUBLIC_NOTIFICATION_VIBRATION=true

# Security
EXPO_PUBLIC_SECURE_STORAGE=true
EXPO_PUBLIC_BIOMETRIC_AUTH=false

# App Store Configuration
EXPO_PUBLIC_APP_STORE_URL=https://apps.apple.com/app/food-rush
EXPO_PUBLIC_PLAY_STORE_URL=https://play.google.com/store/apps/details?id=com.mrcalculus.foodrush

# Support
EXPO_PUBLIC_SUPPORT_EMAIL=support@foodrush.com
EXPO_PUBLIC_SUPPORT_PHONE=+237123456789

# Legal
EXPO_PUBLIC_PRIVACY_POLICY_URL=https://foodrush.com/privacy
EXPO_PUBLIC_TERMS_OF_SERVICE_URL=https://foodrush.com/terms