    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true
