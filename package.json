{
  "name": "food-rush",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "monitor:bundle": "node scripts/monitor-bundle-size.js",
    "bundle:quick": "node scripts/quick-bundle-check.js",
    "bundle:size": "npm run analyze:bundle && npm run monitor:bundle",
    "bundle:export": "expo export --platform all --dev false --clear",
    "bundle:web-analyze": "npx expo export --platform web --dev false && npx webpack-bundle-analyzer dist/_expo/static/js/*.js",
    "deps:analyze": "npx depcheck",
    "deps:unused": "npx depcheck --json | jq '.dependencies'",
    "production:cleanup": "node scripts/production-cleanup.js",
    "production:prepare": "npm run production:cleanup && npm run lint && npm run bundle:size",
    "production:build:android": "NODE_ENV=production eas build --platform android --profile production",
    "production:build:ios": "NODE_ENV=production eas build --platform ios --profile production",
    "production:build:all": "npm run production:prepare && npm run production:build:android && npm run production:build:ios",
    "production:test": "npm run lint && npm run format && npm run production:cleanup",
    "clean": "rm -rf node_modules/.cache dist .expo/web-build production-cleanup-report.json",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "fix:cache": "node scripts/fix-metro-cache.js",
    "fix:syntax": "npm run lint --fix && npm run format",
    "fix:app": "node scripts/fix-app-errors.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^5.1.8",
    "@hookform/resolvers": "^5.2.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/material-top-tabs": "^7.3.4",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/native-stack": "^7.3.23",
    "@react-navigation/stack": "^7.4.8",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "expo": "54.0.13",
    "expo-asset": "~12.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.14",
    "expo-device": "~8.0.9",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.8",
    "i18next": "^25.3.6",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.1",
    "react-native": "0.81.4",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "yup": "^1.6.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-react-constant-elements": "^7.27.1",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@types/react": "~19.1.10",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^9.32.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.12",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
