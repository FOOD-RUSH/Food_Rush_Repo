{
  "name": "food-rush",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "validate:translations": "node src/locales/validate-translations.js validate",
    "validate:qodo": "node .qodo/validate-config.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "monitor:bundle": "node scripts/monitor-bundle-size.js",
    "optimize:customer": "expo export --platform ios --dev false --clear --output-dir dist/customer",
    "optimize:restaurant": "expo export --platform ios --dev false --clear --output-dir dist/restaurant",
    "optimize:icons": "echo 'Icon optimization implemented via Metro config'",
    "optimize:images": "echo 'Image optimization implemented via OptimizedImage component'",
    "optimize:all": "npm run analyze:bundle && npm run monitor:bundle",
    "build:android:dev": "eas build --platform android --profile development --local",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "build:android:apk": "eas build --platform android --profile preview --local"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.8",
    "@hookform/resolvers": "^5.2.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/elements": "^2.6.1",
    "@react-navigation/material-top-tabs": "^7.3.4",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/native-stack": "^7.3.23",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "expo": "53.0.22",
    "expo-asset": "~11.1.7",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-location": "^18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "i18next": "^25.3.6",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-pager-view": "6.7.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-tab-view": "^4.1.2",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.20.0",
